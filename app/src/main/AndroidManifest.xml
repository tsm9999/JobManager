<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kurobarabenjamingeorge.mynavigationdrawer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".AccrualsActivity"></activity>
        <activity android:name=".RejectedOnClick" />
        <activity android:name=".SubmittedOnClick" />
        <activity
            android:name=".JobActivity"
            android:label="@string/title_activity_job"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity
            android:name=".AddJobActivity"
            android:label="Add Job Activity"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity
            android:name=".Overview"
            android:label="Overview"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>